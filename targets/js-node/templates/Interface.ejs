<%- generateApiSummary("    ", datatype, "summary")
%><% if (!datatype.properties || datatype.properties.length == 0) {
%>    export interface <%- datatype.name %><%- getBaseTypeSyntax(datatype) %> {}
<% } else {
%>    export interface <%- datatype.name %><%- getBaseTypeSyntax(datatype) %> {
<% for (var i = 0; i < datatype.properties.length; i++) { var property = datatype.properties[i]
%><%- generateApiSummary("        ", property, "description")
%>        <%- property.name %><%- getPropertyTsType(property, datatype) %>;
<% } %>    }
<% } %>
