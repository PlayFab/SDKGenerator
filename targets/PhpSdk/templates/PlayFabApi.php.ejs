<?php
include 'PlayFabHttp.php';

class PlayFab<%- api.name %>Api
{
<% for(var i in api.calls) { var apiCall = api.calls[i];
%><%- GenerateApiSummary("    ", apiCall, "summary", "https://api.playfab.com/Documentation/" + api.name + "/method/" + apiCall.name)
%>    public static function <%- apiCall.name %>($titleId, $authValue, $request)
    {
<%- GetRequestActions("        ", apiCall, api) %>
        $result = PlayFabHttp::MakeCurlApiCall($titleId, "<%- apiCall.url %>", $request, <%- GetAuthKey(apiCall, api) %>, $authValue);
        return $result;
    }

<% } %>}
?>
